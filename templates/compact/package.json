{
  "name": "{project_name}",
  "version": "0.1.0",
  "description": "{project_name} project",
  "main": "app.js",
  "scripts": {
    "test": "NODE_ENV=\"test\" pm2 start app.js --name={project_name} --watch=. --ignore-watch=\"var docs\"",
    "dev": "NODE_ENV=\"dev\" pm2 start app.js --name={project_name} --watch=. --ignore-watch=\"var docs\"",
    "local.start": "NODE_ENV=local pm2 start app.js --name={project_name} --watch=./ --ignore-watch=\"var docs\"",
    "win.start": "SET NODE_ENV=local&pm2 start app.js --name={project_name} --watch=./ --ignore-watch=\"var docs\"",
    "restart": "pm2 restart app.js",
    "status": "pm2 logs {project_name}"
  },
  "dependencies": {
    "body-parser": "^1.16.0",
    "bootjs": "^0.2.2",
    "bootjs-common": "^0.1.2",
    "bootjs-config": "^0.1.0",
    "bootjs-render": "^0.1.4",
    "co": "^4.6.0",
    "colors": "^1.1.2",
    "connect-redis": "^3.2.0",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "ejs": "^2.5.5",
    "express": "^4.14.1",
    "express-partials": "^0.3.0",
    "express-request-id": "^1.4.0",
    "express-session": "^1.15.0",
    "express-validator": "^2.21.0",
    "file-stream-rotator": "0.0.7",
    "formidable": "^1.1.1",
    "ioredis": "^2.5.0",
    "ioredis-timeout": "^1.3.0",
    "is-type-of": "^1.0.0",
    "joi": "^8.4.2",
    "js-yaml": "^3.8.1",
    "lodash": "^4.17.4",
    "log4js": "^0.6.38",
    "mkdirp": "^0.5.1",
    "moment": "^2.17.1",
    "morgan": "^1.8.0",
    "nunjucks": "^3.0.0",
    "request": "^2.79.0",
    "serve-favicon": "^2.3.2",
    "stack-trace": "0.0.9",
    "validator": "^5.7.0"
  },
  "keywords": [
    "{project_name} project"
  ],
  "author": "guojunlong",
  "license": "MIT"
}
